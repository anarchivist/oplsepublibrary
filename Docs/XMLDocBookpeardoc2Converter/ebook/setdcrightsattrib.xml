<refentry id="package...ebook.setdcrightsattrib">
 <refnamediv>
  <refname>
   <function>ebook::setDcRightsAttrib</function>
  </refname>
  <refpurpose>Set the optional rights attributes.</refpurpose>
 </refnamediv>
 <refsynopsisdiv>
  <funcsynopsis>
   <funcsynopsisinfo>require_once &apos;/ebook.php&apos;;</funcsynopsisinfo>
   <funcprototype>
    <funcdef>
     <replaceable>&true;</replaceable>
     <function>ebook::setDcRightsAttrib</function>
    </funcdef>
    <paramdef>
     $attrib
     <parameter>$attrib</parameter>
    </paramdef>
    <paramdef>
     $attribValue
     <parameter>$attribValue</parameter>
    </paramdef>
    <paramdef>
     $index
     <parameter>
      <optional>$index = &null;</optional>
     </parameter>
    </paramdef>
   </funcprototype>
  </funcsynopsis>
 </refsynopsisdiv>
 <refsect1 id="package...ebook.setdcrightsattrib.desc">
  &title.desc;
  &notdocumented;
</refsect1>
 <refsect1 id="package...ebook.setdcrightsattrib.param">
  &title.param;
  <para>
   <variablelist>
    <varlistentry>
     <term>
      <type>$attrib</type>
      <parameter>$attrib</parameter>
     </term>
     <listitem>
      <para>
       The name of the optional attributes to be set. Optional attributes are
       XmlLang.
</para>
     </listitem>
    </varlistentry>
   </variablelist>
   <variablelist>
    <varlistentry>
     <term>
      <type>$attribValue</type>
      <parameter>$attribValue</parameter>
     </term>
     <listitem>
      <para>
       The value to be set for the optional attribute. If you want set more then
       one rights attribute send  an array into this parameter. Make sure the
       attributes corresponde the appropriate rights index
</para>
     </listitem>
    </varlistentry>
   </variablelist>
   <variablelist>
    <varlistentry>
     <term>
      <type>$index</type>
      <parameter>$index</parameter>
     </term>
     <listitem>
      <para>
       if there is more then one instance of this item, specify it and that item
       will be edited.
</para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 id="package...ebook.setdcrightsattrib.returns">
  &title.returns;
  <para>
   <emphasis>returns</emphasis>
   if set or false if not.
  </para>
 </refsect1>
 <refsect1 id="package...ebook.setdcrightsattrib.throws">
  &title.throws;
  <para>
   <emphasis>throws</emphasis>
   no exceptions thrown
  </para>
 </refsect1>
 <refsect1 id="package...ebook.setdcrightsattrib.note">
  &title.note;
  &note.notstatic;
</refsect1>
</refentry>
