<refentry id="package...ebook.setdctitleattrib">
 <refnamediv>
  <refname>
   <function>ebook::setDcTitleAttrib</function>
  </refname>
  <refpurpose>Set the optional title attributes.</refpurpose>
 </refnamediv>
 <refsynopsisdiv>
  <funcsynopsis>
   <funcsynopsisinfo>require_once &apos;/ebook.php&apos;;</funcsynopsisinfo>
   <funcprototype>
    <funcdef>
     <replaceable>&true;</replaceable>
     <function>ebook::setDcTitleAttrib</function>
    </funcdef>
    <paramdef>
     $attrib
     <parameter>$attrib</parameter>
    </paramdef>
    <paramdef>
     $attribValue
     <parameter>$attribValue</parameter>
    </paramdef>
    <paramdef>
     <parameter>
      <optional>$index = &null;</optional>
     </parameter>
    </paramdef>
   </funcprototype>
  </funcsynopsis>
 </refsynopsisdiv>
 <refsect1 id="package...ebook.setdctitleattrib.desc">
  &title.desc;
  &notdocumented;
</refsect1>
 <refsect1 id="package...ebook.setdctitleattrib.param">
  &title.param;
  <para>
   <variablelist>
    <varlistentry>
     <term>
      <type>$attrib</type>
      <parameter>$attrib</parameter>
     </term>
     <listitem>
      <para>
       The name of the optional attributes to be set. Optional attributes are
       XmlLang.
</para>
     </listitem>
    </varlistentry>
   </variablelist>
   <variablelist>
    <varlistentry>
     <term>
      <type>$attribValue</type>
      <parameter>$attribValue</parameter>
     </term>
     <listitem>
      <para>The value to be set for the optional attribute.</para>
     </listitem>
    </varlistentry>
   </variablelist>
   <variablelist>
    <varlistentry>
     <term>
      <type />
      <parameter>$index</parameter>
     </term>
     <listitem>
      <para />
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 id="package...ebook.setdctitleattrib.returns">
  &title.returns;
  <para>
   <emphasis>returns</emphasis>
   if set or false if not.
  </para>
 </refsect1>
 <refsect1 id="package...ebook.setdctitleattrib.throws">
  &title.throws;
  <para>
   <emphasis>throws</emphasis>
   no exceptions thrown
  </para>
 </refsect1>
 <refsect1 id="package...ebook.setdctitleattrib.note">
